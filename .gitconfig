[user]
	name = Andreas Lindh√©
	email = Lindhe94@gmail.com
[alias]
	bra = branch
	cl = clone
	co = checkout
	com = commit -v
	coma = commit -av
	comam = commit -am
	comm = commit -m
	df = diff --color-words
	dif = diff
	difftex = difftool -y -t latex
	h = help
	l = log --oneline --decorate
	lg = log --oneline --decorate
	log = log
	reb = rebase
	rem = remote
	s = status
	sho = show
	st = status
[branch]
	autosetuprebase = always
[pull]
	rebase = true
[credential]
	helper = cache
[core]
	excludesfile = ~/.gitignore_global
[push]
	default = simple
[color]
	status = always
